#include <stdio.h>

int main(){
    int i;
    int j;
    int n;
    scanf("%d\n", &n);
    int m;
    scanf("%d\n", &m);
    int a[n][m];

    for (i=0; i<n; i++){
        for (j=0; j<m; j++){
            scanf("%d", &a[i][j]);
        }
    }
    
    for (i=0; i<n; i++){
        for (j=0; j<m; j++){
            printf("%d", (-1) *  a[i] [j]);
        }
        printf("\n");
    }

    return 0;
}
